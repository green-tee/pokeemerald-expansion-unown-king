script(local) MoveTutor_EventScript_SelectMon {
    msgbox(MoveTutor_Text_WhichMon, MSGBOX_DEFAULT)
    closemessage
    special(ChooseMonForMoveTutor)
	waitstate
    if (var(VAR_RESULT) == 0) {
        msgbox(MoveTutor_Text_ComeBackIfYouNeedMe, MSGBOX_DEFAULT)
        closemessage
    } else {
        msgbox(MoveTutor_Text_DoneComeBackIfYouNeedMe, MSGBOX_DEFAULT)
        closemessage
    }
}

text MoveTutor_Text_TeachSomeMove {
    "Hello, traveler.\n"
    "Would you like to teach a move to\l"
    "one of your Pokémon?\l"
    "I'm not going to charge you for it."
}

text MoveTutor_Text_WhichMove {
    "Okay, which one?"
}

text Multichoice_Option_NoMove {
    "I've changed my mind."
}

text MoveTutor_Text_WhichMon {
    "Which Pokémon should learn this move?"
}

text MoveTutor_Text_ComeBackIfYouNeedMe {
    "Nevermind then.\n"
    "Come back if you need me."
}

text MoveTutor_Text_DoneComeBackIfYouNeedMe {
    "Cool, isn't it?\n"
    "Come back if you need me again!"
}
